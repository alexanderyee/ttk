func _ready():
	num_words = WORDS.size()
	
func get_random_word() -> String:
	var num_attempts := 0
	var new_word: String
	var new_word_found := false
	while num_attempts < MAX_RNG_WORD_ATTEMPTS and not new_word_found:
		new_word = WORDS[Global.rng.randi_range(0, num_words - 1)]
		# check if first letter matches any existing words' first letters
		new_word_found = true
		for word in existing_words:
			if word[0] == new_word[0]:
				new_word_found = false
		
	if not new_word_found:
		push_warning("Couldn't find a good word to pick...")
	return new_word

func _on_enemy_spawner_word_added(enemy: Enemy, word: String):
	existing_words.append(word)

func _on_enemy_word_panel_word_typed(word: String):
	existing_words.erase(word)
